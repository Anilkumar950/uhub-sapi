/report:
  get:
    description: to get covid report.
    headers:
      client_id:
        type: string
        required: true
        description: client id to authenticate to system API
      client_secret:
        type: string
        required: true
        description: client secret to authenticate to system API
      x-transaction-id:
        type: string
        required: false
        description: transaction id to maintain over APIs
      x-correlation-id:
        type: string
        required: false
        description: correlation id to maintain over APIs
    queryParameters:
      state:
        type: string
        required: false
        displayName: state
        example: "texas"
    responses:
      200:
        body:
          application/json:
            example:
              [{
                "state": "texas",
                "report":{
                  "total": "57668",
                  "positive": "454367",
                  "recovered": "54678",
                  "death": "300",
                  "vaccinated": 78906587
              } 
              }]
      500:
        body:
          application/json:
            example:
              {
                "code": 500,
                "Message": "Internal server error",
                "decription": "The server encountered an unexpected condition which prevented it from fulfilling the request",
                "DateTime": "2021-05-28T05:58:02",
                "TransactionID": "4b4546-9r54-6yt5r-7yujh"
              }
      503:
        body:
          application/json:
            example:
              {
                "code": 503,
